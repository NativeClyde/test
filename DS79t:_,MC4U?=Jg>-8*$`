local u = {
{"CPygtf", "test"}
}
local n = game.Players.LocalPlayer.Name
local k, i = false, n == e
for _, v in ipairs(u) do
    if e == v[1] then
        k = true
        break
    end
end
local m = (not k or not i) and "Invalid" or ""
if k and i then
    wait(2)
local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()

local GUI = Mercury:Create{
    Name = "Mercury",
    Size = UDim2.fromOffset(650, 450),
    Theme = Mercury.Themes.Dark,
    Link = "https://github.com/deeeity/mercury-lib"
}

GUI:Notification{
    Title = "AOT: FW \\ v0.2",
    Text = "(Delete) To toggle UI",
    Duration = 10,
    Callback = function() 
        wait(2)
    for _,v in pairs(getconnections(game:GetService("ScriptContext").Error)) do
         v:Disable()
     end
     
    for _,v in pairs(getconnections(game:GetService("LogService").MessageOut)) do
         v:Disable()
     end    
    end
}

local Tab = GUI:Tab{
    Name = "Freedom War",
    Icon = "rbxassetid://8569322835"
}
GUI:Credit{
    Name = "Credits",
    Description = ".automic (Script), jagt#5941 (Script), laserpope(hes him)",
    V3rm = nil,
    Discord = nil,
}

Tab:Button{
    Name = "Anti Bypass",
    Description = "Press this before anything",
    Callback = function() 
        hookfunction(debug.info, function()end)

        function infWait(...) return task.wait(math.huge); end
        
        for _, f in ipairs(getgc()) do
            if not islclosure(f) then continue end
        
            for _, c in ipairs(debug.getconstants(f)) do
                if type(c) == "string" and c:lower():find("adonis anti cheat") then
                    hookfunction(f, infWait);
                end
            end
        end
    end
}

Tab:Button{
    Name = "Damage Mod",
    Description = "If damage is under 600 it will auto hit 670",
    Callback = function() 
local a
        a = hookmetamethod(game, "__namecall", function(b, ...)
    if b.Name == "HitEvent" and getnamecallmethod() == "FireServer" then
        local c = {...}
        local d = c[2]
    if d < 600 then
                            c[2] = 670 -- If damage is under 600
    elseif d > 1170 then
                            c[2] = 1170 -- If damage is over 1170
    end
        return a(b, unpack(c))
    end
        return a(b, ...)
    end)    
end
}

Tab:Button{
    Name = "Infinite Yield",
    Description = nil,
    Callback = function() 
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end
}

Tab:Button{
    Name = "Warrior Names",
    Description = "Notifies you of current warriors",
    Callback = function() 
    local n=""
    local v={"Colossal","Female","Armored"}
    for _,s in pairs(workspace.WarriorsCandidatesFolder.ShifterHolders:GetChildren())do
    local m=s.Name
    local x=s.Value
    if table.find(v,m)then
    n=n..m..":"..tostring(x).."\n"
    end
    end
    game.StarterGui:SetCore("SendNotification",{
    Title="Warriors",
    Text=n,
    Duration=10,
    })
end
}

local Toggled = false

Tab:Keybind{
    Name = "Spam Blade (ur not levi)",
    Keybind = nil,
    Description = "Will spam M1s (Human PVP)",
    Callback = function(Value)
        Toggled = not Toggled
        local nf = Toggled and "Enabled" or "Disabled"
        local C = game.Players.LocalPlayer.Character

        game.StarterGui:SetCore("SendNotification", 
            {
                Title = "Ackerman Spec",
                Text = nf,
                Icon = "",
                Duration = 0.5,
            })

        while Toggled do
            if C and C:FindFirstChild("Humanoid") and C:FindFirstChild("Gear") then
                local args = {[1] = 2}
                game:GetService("Players").LocalPlayer.Character.Gear.Events.AttackingEvent:FireServer(unpack(args))
                wait(0.01)
            else
                Toggled = false
                game.StarterGui:SetCore("SendNotification", 
                 {
                    Title = "Ackerman Spec",
                    Text = "ODM Required",
                    Icon = "",
                    Duration = 2,
                 })
            end
        end
    end
}

Tab:Toggle{
    Name = "Inf Blade/Gas",
    StartingState = false,
    Description = "Includes Max Hookrange, Sanity, ATK speed(No trail), Run Speed, God Mode from (Titans)",
    Callback = function(state) 
        while state do
            local C = game.Players.LocalPlayer.Character
            if C and C:FindFirstChild("Humanoid") then
                local Gear = C.Humanoid:FindFirstChild("Gear")
                if Gear then
                    C.Humanoid.Invinsible.Value = true
                    C.Humanoid.Sanity.Value = 100
                    Gear.Upgrades.GasEfficiency.Value = 0.01
                    Gear.Blades.Value = 8
                    Gear.Upgrades.AttackSpeed.Value = 0.2
                    Gear.Upgrades.RunningSpeed.Value = 19
                    Gear.Upgrades.MentalStrength.Value = 0.3
                    Gear.Upgrades.HooksRange.Value = 160
                end
            end
            wait(1)
        end
    end
}

Tab:Textbox{
    Name = "Features under this list must be enabled each spawn",
    Callback = function(text) end
}

Tab:Button{
    Name = "Unlock All Skills",
    Description = nil,
    Callback = function() 
mykid = game.Players.LocalPlayer.Character.Humanoid.Gear.Skills:GetChildren()

        for _, mykid in pairs(mykid) do
            if mykid:IsA("BoolValue") then
                mykid.Value = true
            end
        end 
    end
}

Tab:Button{
    Name = "No CD Skills",
    Description = "Warning: Spamming moves might cause a kick",
        Callback = function() 
            while true do
            local a = game.Players.LocalPlayer.Character.Gear.SkillsSpamLimit:GetChildren()
            for _, b in pairs(a) do
            if b:IsA("IntValue") then
            b.Value = 0
            end
            end
            local c = game.Players.LocalPlayer.PlayerGui.SkillsGui
            for _, d in pairs(c:GetChildren()) do
            if d:FindFirstChild("Cooldown") then
            d.Cooldown.Value = 10000
            end
            end
            wait(0.2)
        end    
    end
}
else
game.Players.LocalPlayer:Kick(m)
end
